{
  "version": "2.3.4",
  "compilerOptions": {
    "module": "system",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": false,
    "target": "es5",
    "paths": {
      "app/*": [
        "jspmAassets/app/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "static",
    "jspm_packages"
  ]
}